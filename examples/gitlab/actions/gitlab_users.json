        "provider": {
          "type": "string",
          "description": "External provider name.",
          "required": false
        },
        "public_email": {
          "type": "string",
          "description": "Public email of the user (must already be verified).",
          "required": false
        },
        "shared_runners_minutes_limit": {
          "type": "integer",
